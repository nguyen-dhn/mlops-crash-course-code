pipeline {
    agent any

    stages {
        stage('Build') {
            steps {
                echo '[build] Building something..'
            }
        }
        stage('Test') {
            steps {
                echo '[test] Testing something..'
            }
        }
        stage('Deploy') {
            steps {
                echo '[deploy] Deploying something..'
            }
        }
    }
}
